{
    "name": "PilotPipe",
    "inputPorts": {},
    "outputPorts": {
        "IMAGE": {
            "type": "dwImageHandle_t"
        },
        "IMAGE_TIMESTAMP": {
            "type": "dwTime_t"
        },
        "VALUE_0":{
            "type": "int"
        },
        "GUARDIAN_INSTRUCT": {
            "type": "gwGuardianInstruct"
        }
    },
    "parameters": {
        "cameraIndex": {
            "type": "size_t",
            "default": 0
        },
        "cameraType": {
            "type": "gwCameraType",
            "default": "GW_CAMERA_INVALID"
        },
        "cameraParameter": {
            "type": "dw::core::FixedString<256>",
            "default": "invalid"
        },
        "cameraProtocol": {
            "type": "dw::core::FixedString<256>",
            "default": "camera.virtual"
        },
        "cameraIntrinsics": {
            "type": "dw::core::FixedString<256>",
            "default": "invalid"
        },
        "preprocMode": {
            "type": "gwPreprocMode",
            "default": "GW_PREPROC_INVALID"
        }
    },
    "subcomponents": {
        "cameraCapture": {
            "componentType": "../camera/CameraCapture.graphlet.json",
            "parameters": {
                "cameraIndex": "$cameraIndex",
                "cameraType": "$cameraType",
                "cameraParameter": "$cameraParameter",
                "cameraProtocol": "$cameraProtocol",
                "cameraIntrinsics": "$cameraIntrinsics"
            }
        },
        "cameraPreproc": {
            "componentType": "../preproc/PilotPreproc.graphlet.json",
            "parameters": {
                "*": "$*"
            }
        }
    },
    "connections": [
        {
            "src": "cameraCapture.VALUE_0",
            "dests": {
                "cameraPreproc.VALUE_IN": {}
            }
        },
        {
            "src": "cameraCapture.GUARDIAN_INSTRUCT",
            "dests": {
                "GUARDIAN_INSTRUCT": {}
            }
        },
        {
            "src": "cameraPreproc.VALUE_OUT",
            "dests": {
                "VALUE_0": {}
            }
        }
    ]
}